<FindBugsFilter>
  <Match>
    <Or>
      <Class name="~android.*" />
      <Class name="~org.json.*" />
      <Class name="~.*\.R\$.*" />
      <Class name="~.*\.Manifest\$.*" />
    </Or>
    <Bug category="PERFORMANCE,MALICIOUS_CODE,STYLE,SECURITY,CORRECTNESS,MT_CORRECTNESS,BAD_PRACTICE,I18N" />
  </Match>
</FindBugsFilter>
